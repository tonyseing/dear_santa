  <!-- ========== CONTACT US ========== -->
  <section id="contact">
    <div class="container">

      <div class="row-fluid">
        <div class="span12 offset0">
          <h3>Send Mail To Santa Claus</h3>

          <form id="child_form_mailer" action="/santa/write" method="post">
            <div class="" style="float: left;
                                 width: 48%;
                                 margin-right: 4%" >
              <label for="name">Your First Name</label>
              <input style="width: 100%;" class="span6" name="message[firstname]" id="name" value="" type="text">
            </div>
            <div class="" style="float: left;
                                 width: 48%; ">
              <label for="name">Your Last Name</label>
              <input style="width: 100%;" class="span6" name="message[lastname]" id="name" value="" type="text">
            </div>

            <div class="" style="float: left;
                                 width: 48%;
                                 margin-right: 4%">
              <label for="name">Your Email Address</label>
              <input style="width: 100%;" style="width: 100%;" class="span12" name="message[email]" id="email" value="" type="text">
            </div>
            <div class="" style="float: left;
                                 width: 48%;">
              <label for="name">Your Mom or
                your Dad's Email Address</label>
              <input style="width: 100%;" class="span12" name="message[parent_email]" id="parent_email" value="" type="text">
            </div>
            <div class="">
              <label for="msg">Your Message</label>
              <textarea class="span12"
                        name="message[content]" id="content" style="height:
                                                                    400px;"></textarea>
            </div>
            <input class="btn btn-large"
                   type="submit" value="Send Santa Mail">
          </form>
        </div>
      </div>
    </div>
